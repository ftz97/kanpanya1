{
  "name": "padavwa",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "next dev -p 3000",
    "mobile": "HOST=$(npx local-ip-url) next dev -H 0.0.0.0 -p 3000",
    "prebuild": "bash scripts/check-imports.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .ts,.tsx --max-warnings=100 .",
    "lint:fix": "eslint --ext .ts,.tsx --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "generate-e2e-env": "node scripts/generate-e2e-env.js",
    "generate-test": "node scripts/generate-test.js",
    "generate-test:simple": "node scripts/generate-test-simple.js",
    "generate-test:advanced": "node scripts/generate-test-advanced.js",
    "create-test-user": "npx tsx scripts/create-test-user.ts",
    "test:e2e": "playwright test -c playwright.logged.config.ts",
    "test:e2e:client": "playwright test -c playwright.e2e.config.ts",
    "test:e2e:ui": "playwright test -c playwright.e2e.config.ts --ui",
    "test:e2e:check": "node scripts/test-e2e-quick.js",
    "test:e2e:ci": "playwright test -c playwright.ci.config.ts",
    "test:e2e:dot": "playwright test --reporter=dot",
    "test:last": "npx playwright test $(cat .last-test)",
    "ci:check": "pnpm run lint && pnpm run test:e2e:dot",
    "test:quality": "node scripts/test-quality-gate.js",
    "typecheck": "tsc --noEmit",
    "check:i18n": "npx tsx scripts/check-i18n.ts",
    "i18n:heal": "npx tsx scripts/i18n-heal.ts",
    "i18n:heal:copyfr": "npx tsx scripts/i18n-heal.ts --strategy=copy-fr",
    "i18n:heal:onlyes": "npx tsx scripts/i18n-heal.ts --locales=es",
    "i18n:heal:auth": "npx tsx scripts/i18n-heal.ts --domains=auth",
    "i18n:merge-common": "npx tsx scripts/merge-common-errors.ts",
    "i18n:merge-common:dry": "npx tsx scripts/merge-common-errors.ts --dry-run",
    "test:i18n": "npx tsx scripts/test-i18n-manual.ts",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@hookform/resolvers": "^5.2.1",
    "@mapbox/mapbox-gl-draw": "1.4.3",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@playwright/test": "^1.55.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.1",
    "@types/d3": "^7.4.3",
    "@types/d3-sankey": "^0.12.4",
    "@types/react-map-gl": "^6.1.7",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "js-confetti": "^0.13.1",
    "lucide-react": "^0.542.0",
    "mapbox-gl": "2.15.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "openai": "^5.20.0",
    "react": "19.1.0",
    "react-confetti": "^6.4.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-map-gl": "7.1.7",
    "react-scratchcard": "^1.1.2",
    "react-scratchcard-v2": "^1.1.1",
    "react-select": "^5.10.2",
    "react-use": "^17.6.0",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.6",
    "@storybook/addon-docs": "^9.1.6",
    "@storybook/addon-onboarding": "^9.1.6",
    "@storybook/addon-vitest": "^9.1.6",
    "@storybook/nextjs": "^9.1.6",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.1.5",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "^9.1.6",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "local-ip-url": "^1.0.10",
    "only-allow": "^1.2.1",
    "postcss": "^8.5.6",
    "storybook": "^9.1.6",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5"
  }
}
